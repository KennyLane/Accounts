<%= simple_form_for(@nomtran) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :tran %>
    <%= f.association :nominal %>
    <%= f.input :date %>
    <%= f.input :amount %>
    <%= f.input :type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
